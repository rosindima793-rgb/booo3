ORACLE_PK=0xYOUR_ORACLE_PRIVATE_KEY
RPC_URL=https://rpc.monad.xyz
CORE_PROXY=0x... # core contract address
WBNB=0x... # wrapped MON token address
PANCAKE_ROUTER=0x... # pancake router
PANCAKE_FACTORY=0x... # pancake factory
BOT_ACCOUNT=0x... # public address of ORACLE_PK
